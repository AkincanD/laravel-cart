{
  "name": "lukepolo/laracart",
  "description": "A simple cart for Laravel",
  "keywords": [
    "laravel",
    "cart",
    "shopping",
    "shopping cart"
  ],
  "homepage": "https://github.com/lukepolo/laracart",
  "license": "MIT",
  "authors": [
    {
      "name": "Luke Policinski",
      "email": "Luke@LukePOLO.com",
      "homepage": "http://LukePOLO.com"
    }
  ],
  "require": {
    "php": "~7.2 || ~7.3 || ~7.4 || ~8.0",
    "ext-intl" : "*",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/session": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/events": "^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "~v6.17.0",
    "mockery/mockery": "^1.0",
    "orchestra/database": "~v6.0.0"
  },
  "autoload": {
    "psr-4": {
      "LukePOLO\\LaraCart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LukePOLO\\LaraCart\\Tests\\": "tests"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "LukePOLO\\LaraCart\\LaraCartServiceProvider"
          ],
          "aliases": {
              "LaraCart": "LukePOLO\\LaraCart\\Facades\\LaraCart"
          }
      }
  },
  "minimum-stability": "stable"
}
